
@array_system=Array.new(14)

@array_system.each do |value|
        random = Random.rand(0.0..1.0)
        if random<=0.2
          @array_system.push='O'
        elsif random<=0.6
          @array_system.push='X'
        else
          @array_system.push=' ' 
        end
        
end

p @array_system


