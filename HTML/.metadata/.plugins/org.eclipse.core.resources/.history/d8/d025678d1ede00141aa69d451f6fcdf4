class Baba
   def initialize (name)
       @name= name
       puts "#{@name} #{self}"
   end
   def to_s
      "Name is #{@name}"
   end
end

p Baba.to_s

pa= Baba.new("Jone")
puts pa


class Son<Baba
   
end

papa=Son.new("joe")
puts Son.superclass
puts Baba.superclass
puts Baba.superclass.superclass
puts Baba.superclass.superclass.superclass