class Baba
   def initialize (name)
       @name= name
       puts "#{@name} #{self}"
   end
   def to_s
      "Name is #{@name}"
   end
end

puts Baba

pa= Baba.new("Jone")
puts pa


class Son<Baba

    def to_s
      super
    end
end

papa=Son.new("joe")
  
p papa

 
=begin
puts Son.superclass
puts Baba.superclass
puts Baba.superclass.superclass
puts Baba.superclass.superclass.superclass
=end