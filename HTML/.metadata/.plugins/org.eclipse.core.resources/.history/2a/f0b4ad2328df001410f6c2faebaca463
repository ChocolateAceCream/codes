class Person
    include Comparable
    attr_reader:name
    def initialize (name,iq)
     @name=name
     @iq=iq
    end
    
    def to_s
        "#{@iq}"
    end
    
    def <=>(other)
        self.name<=>other.name
    end   
end

p1 = Person.new("Matz",54)
p2 = Person.new("Guido",65)
p3 = Person.new("Larry",76)

if  p1>p2
    p p1
    p p2
    puts "#{p1.name}>#{p2.name}"
end

puts "sorted"
puts [p1,p2,p3].sort
