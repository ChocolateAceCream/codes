class Node
    attr_accessor :current_node, :next_node
    def initialize(val, next_val)
        @current_node = val
        @next_node = next_val
    end
end

class Link_list
  
    def initialize(val)
      @head = Node.new(val, nil)
    end
    
    def add(value)
        current = @head
        while(current.next_node != nil)
            current.current_node = current.next_node
        end
        current.next = Node.new(val,nil) 
    end 
    
