
total=0

puts "Please input your Mid-term grades=> "
midTerm = Float(gets)
total = midTerm*0.2
puts "Please input your Project grades=> "
project=Float(gets)
if project<midTerm
  project=midTerm
end
total +=project*0.2

puts "Please input your HW1 grades=> "
hw1= Float(gets)
min=hw1
total+=hw1*0.075
puts "Please input your HW2 grades=> "
hw2= Float(gets)
if hw2<min
  min=hw2
end
total+=hw2*0.075
puts "Please input your HW3 grades=> "
hw3= Float(gets)
if hw3<min
  min=hw3
end
total+=hw3*0.075
puts "Please input your HW4 grades=> "
hw4= Float(gets)
if hw4<min
  min=hw4
end
total+=hw4*0.075
puts "Please input your HW5 grades=> "
hw5= Float(gets)
if hw5<min
  min=hw5
end
total+=hw5*0.075
total-=min*0.075
puts "Please input your final exam grades=> "
final = Float(gets)
total+=final*0.4
printf("Your final grade is %6.2f",total)
