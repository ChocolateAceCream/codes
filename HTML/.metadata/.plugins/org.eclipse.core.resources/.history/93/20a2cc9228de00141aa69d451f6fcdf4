class Baba
   def initialize (name)
       @name= name
       puts "#{@name} #{self}"
   end
   def to_s
      "Name is #{@name}"
   end
end

puts Baba

pa= Baba.new("Jone")
puts pa


class Son<Baba

    def to_s
      super
    end
end

papa=Son.new("joe")
  
p papa

File.open("test.log") do |log|
  log.seek(-500,IO::SEEK_END) # back up 500 characters from end
  log.gets                    # ignore partial line
  log.read                    # and return rest
end
=begin
puts Son.superclass
puts Baba.superclass
puts Baba.superclass.superclass
puts Baba.superclass.superclass.superclass
=end