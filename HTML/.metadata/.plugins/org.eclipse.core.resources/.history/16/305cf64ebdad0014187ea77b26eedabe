class Animal
  def animal_call
    protect_me
  end
  protected
  def protect_me
    p "protect_me called from #{self.class}"
  end  
end

n1= Animal.new
n1.animal_call #=> protect_me called from Animal
#n1.protect_me #=>error: protected method `protect_me' called

class Mammal < Animal
  def mammal_call
    protect_me
  end
end 

puts "n2"
n= Mammal.new
n.mammal_call #=> protect_me called from Mammal
