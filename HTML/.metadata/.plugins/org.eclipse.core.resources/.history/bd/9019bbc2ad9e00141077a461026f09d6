<!-- change color -->
<p style = "color:yellow">color</p>

<!-- change both color and font size -->

<p style = "color:yellow;font-size: 42px">color</p>

<!-- change the font style -->
<ol>
<li style="font-family: Arial">Hello!</li>
<li style="font-family: Impact">Hello!</li>
<li style="font-family: Verdaa">Hello!</li>
</ol>

<!-- change the background color -->
<p style = "background-color:yellow">color</p>

<!-- align the text -->
<ol>
<li style="text-align:center">Hello!</li>
<li style="text-align:right">Hello!</li>
<li style="text-align:left">Hello!</li>
</ol>


